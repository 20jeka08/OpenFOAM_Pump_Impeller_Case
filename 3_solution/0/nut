FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    Inlet
    {
        type            calculated;
        value           uniform 0;
    }

    Outlet
    {
        type            calculated;
        value           uniform 0;
    }

    Wall
    {
        // Mandatory entries (unmodifiable)
        type            nutkRoughWallFunction;
        Ks              uniform 0.0;
        Cs              uniform 0.75;
        value uniform 0;
    }


    Per_1
    {
        type            cyclicAMI;
    }

    Per_2
    {
        type            cyclicAMI;
    }

}


