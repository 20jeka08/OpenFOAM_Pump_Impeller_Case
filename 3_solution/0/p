FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 101.6298;

boundaryField
{

    Inlet
    {
        type            zeroGradient;
    }
    
    Outlet
    {
        type            fixedValue;
        value           uniform 101.6298;  
    }

    Wall
    {
        type            zeroGradient;
    }

    Per_1
    {
        type            cyclicAMI;
    }

    Per_2
    {
        type            cyclicAMI;
    }
}

